{
    "name": "chrismou/phergie-irc-plugin-react-google",
    "description": "Phergie plugin for returning Google search results and estimated result counts",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "homepage": "http://github.com/chrismou/phergie-irc-plugin-react-google",
    "license": "BSD-2-Clause",
    "support": {
        "email": "phergie@mou.me.uk",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/chrismou/phergie-irc-plugin-react-google/issues",
        "source": "https://github.com/chrismou/phergie-irc-plugin-react-google"
    },
    "minimum-stability": "dev",
    "require": {
		"php": ">=5.4.0",
        "phergie/phergie-irc-bot-react": "dev-master",
        "phergie/phergie-irc-plugin-react-command": "1.0.*",
		"wyrihaximus/phergie-http": "~1.0|~2.0"

    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phake/phake": "2.0.0-beta2",
		"phergie/phergie-irc-plugin-react-autojoin": "1.0.*",
        "phergie/phergie-irc-plugin-react-commandhelp": "dev-master"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
			"Chrismou\\Phergie\\Plugin\\Google\\": "src"
		}
    },
    "autoload-dev": {
        "psr-4": {
            "Chrismou\\Phergie\\Tests\\Plugin\\Google\\": "tests"
        }
    }
}
